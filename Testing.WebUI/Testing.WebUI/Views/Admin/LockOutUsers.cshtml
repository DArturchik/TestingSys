@model Testing.Domain.Entities.User
@{
    ViewBag.Title = "LockOutUsers";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

    <html xmlns="http://www.w3.org/1999/xhtml">
    <head>
        <title></title>
    </head>
    <body >

        <main class="container">

            <h2>Block client.</h2>
            <hr />
            <p>You really want to block the client.</p>
            @using (Html.BeginForm())
            {

                <fieldset>
                    <p>@Html.HiddenFor(x=>x.FirstName)</p>
                    <p>@Html.HiddenFor(x=>x.LastName)</p>
                    <p>@Html.HiddenFor(x=>x.Email)</p>
                    <p>@Html.HiddenFor(x=>x.Password)</p>
                    <input class="btn" type="submit" name="name" value="Lockout" />
                </fieldset>
            }
        </main>
        <div style="height:210px;"></div>


    </body>
    </html>

    